CREATE TABLE matches (
    tourney_id text,
    tourney_name text,
    surface text,
    draw_size smallint,
    tourney_level text,
    tourney_date date,
    match_num smallint,
    winner_id bigint,
    winner_seed smallint,
    winner_entry text,
    winner_name text,
    winner_hand char(1),
    winner_ht smallint,
    winer_ioc varchar(10),
    winner_age double precision,
    loser_id bigint,
    loser_seed smallint,
    loser_entry text,
    loser_name text,
    loser_hand char(1),
    loser_ht smallint,
    loser_ioc varchar(10),
    loser_age double precision,
    score text,
    best_of smallint,
    round varchar(10),
    minutes smallint,
    w_ace smallint,
    w_df smallint,
    w_svpt smallint,
    w_1stIn smallint,
    w_1stWon smallint,
    w_2ndWon smallint,
    w_SvGms smallint,
    w_bpSaved smallint,
    w_bpFaced smallint,
    l_ace smallint,
    l_df smallint,
    l_svpt smallint,
    l_1stIn smallint,
    l_1stWon smallint,
    l_2ndWon smallint,
    l_SvGms smallint,
    l_bpSaved smallint,
    l_bpFaced smallint,
    winner_rank smallint,
    winner_rank_points bigint,
    loser_rank smallint,
    loser_rank_points bigint
);